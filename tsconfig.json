{
  "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "forceConsistentCasingInFileNames" : true,
      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es6",
      "outDir": "./dist",
      "baseUrl": "./src",
      "strict": true,
      "strictNullChecks":false
  },
  "include": [
      "**/*.ts",
  ],
  "exclude": [
      "node_modules"
  ]
}